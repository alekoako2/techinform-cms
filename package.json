{
  "name": "techinform-cms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en -- --port=4201",
    "start:ka": "ng serve --configuration=ka -- --port=4200",
    "start:both": "yarn start:ka && yarn start:en",
    "build": "ng build --prod --i18n-locale ka --output-path=dist/cms/ka --baseHref /ka/",
    "build:en": "ng build --configuration=en",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "translate": "ng xi18n --i18n-locale ka --output-path src/locale",
    "build-locale:en": "ng build --prod --i18n-locale en --i18n-format xlf --i18n-file src/locale/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale": "npm run build && npm run build-locale:en",
    "generate": "apollo client:codegen --queries='src/app/core/authentication/**/*.ts' --target=typescript",
    "schema": "npx apollo-codegen introspect-schema http://localhost:4000/graphql --output ./src/app/types/schema.json",
    "generate-types": "npx apollo-codegen generate src/app/**/**/**/**/*.ts --schema ./src/app/types/schema.json --target typescript --output ./src/app/types/operation-result-types.ts",
    "gen-types": "yarn schema && yarn generate-types"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.2",
    "@angular/cdk": "^8.1.3",
    "@angular/common": "~8.2.2",
    "@angular/compiler": "~8.2.2",
    "@angular/core": "~8.2.2",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.2.2",
    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "~8.2.2",
    "@angular/platform-browser-dynamic": "~8.2.2",
    "@angular/router": "~8.2.2",
    "@ngxs/form-plugin": "^3.5.0",
    "@ngxs/store": "^3.5.0",
    "apollo-angular": "^1.6.0",
    "apollo-angular-link-http": "^1.8.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "hammerjs": "^2.0.8",
    "ng2-file-upload": "^1.3.0",
    "ngx-quill": "^7.3.4",
    "quill": "^1.3.7",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular/cli": "~8.2.2",
    "@angular/compiler-cli": "~8.2.2",
    "@angular/language-service": "~8.2.2",
    "@ngxs/cli": "^1.0.0",
    "@ngxs/devtools-plugin": "^3.5.0",
    "@ngxs/logger-plugin": "^3.5.0",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "codelyzer": "^5.0.0",
    "eslint": "^6.2.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.3.0",
    "typescript": "~3.5.3"
  }
}
